{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/CarConfig",
    "definitions": {
        "CarConfig": {
            "required": [
                "aero",
                "wheelFront",
                "wheelRear",
                "damperFront",
                "damperRear",
                "wheelCompound",
                "rideHeight",
                "balancing",
                "notes"
            ],
            "properties": {
                "aero": {
                    "$ref": "#/definitions/PbConfigs.Aero",
                    "additionalProperties": false
                },
                "wheelFront": {
                    "$ref": "#/definitions/PbConfigs.Wheel",
                    "additionalProperties": false
                },
                "wheelRear": {
                    "$ref": "#/definitions/PbConfigs.Wheel",
                    "additionalProperties": false
                },
                "damperFront": {
                    "$ref": "#/definitions/PbConfigs.Damper",
                    "additionalProperties": false
                },
                "damperRear": {
                    "$ref": "#/definitions/PbConfigs.Damper",
                    "additionalProperties": false
                },
                "wheelCompound": {
                    "type": "string"
                },
                "rideHeight": {
                    "type": "number"
                },
                "balancing": {
                    "type": "string"
                },
                "notes": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Car Config"
        },
        "PbConfigs.Aero": {
            "required": [
                "angleOfIncidenceFront",
                "angleOfIncidenceRear",
                "flap"
            ],
            "properties": {
                "angleOfIncidenceFront": {
                    "type": "number"
                },
                "angleOfIncidenceRear": {
                    "type": "number"
                },
                "flap": {
                    "type": "string"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Aero"
        },
        "PbConfigs.Damper": {
            "required": [
                "bound",
                "rebound",
                "preload"
            ],
            "properties": {
                "bound": {
                    "type": "number"
                },
                "rebound": {
                    "type": "number"
                },
                "preload": {
                    "type": "number"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Damper"
        },
        "PbConfigs.Wheel": {
            "required": [
                "camber",
                "toe"
            ],
            "properties": {
                "camber": {
                    "type": "number"
                },
                "toe": {
                    "type": "number"
                }
            },
            "additionalProperties": false,
            "type": "object",
            "title": "Wheel"
        }
    }
}